<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:WpfApp1.Resources.Style.Dark">


    <SolidColorBrush x:Key="CommonControls.ButtonTextActive" Color="#1E1E1E"/>
    <SolidColorBrush x:Key="CommonControls.ButtonTextDisabled" Color="#A2A4A5"/>

    <SolidColorBrush x:Key="CommonControls.Button" Color="#ECECF0"/>
    <SolidColorBrush x:Key="CommonControls.ButtonBorder" Color="#CCCEDB"/>

    <SolidColorBrush x:Key="CommonControls.ButtonDisabled" Color="#F5F5F5"/>
    <SolidColorBrush x:Key="CommonControls.ButtonBorderDisabled" Color="#CCCEDB"/>

    <SolidColorBrush x:Key="CommonControls.ButtonHover" Color="#C9DEF5"/>
    <SolidColorBrush x:Key="CommonControls.ButtonBorderHover" Color="#3399FF"/>

    <SolidColorBrush x:Key="CommonControls.ButtonPressed" Color="#007ACC"/>
    <SolidColorBrush x:Key="CommonControls.ButtonBorderPressed" Color="#007ACC"/>

    <SolidColorBrush x:Key="CommonControls.ButtonFocused" Color="#C9DEF5"/>
    <SolidColorBrush x:Key="CommonControls.ButtonBorderFocused" Color="#3399FF"/>


</ResourceDictionary>