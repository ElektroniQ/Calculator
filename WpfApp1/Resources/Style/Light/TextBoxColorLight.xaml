<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:WpfApp1.Resources.Skins.Light">

    <SolidColorBrush x:Key="CommonControls.TextBoxBackground" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="CommonControls.TextBoxBorder" Color="#CCCEDB"/>
    <SolidColorBrush x:Key="CommonControls.TextBoxText" Color="#1E1E1E"/>

    <SolidColorBrush x:Key="CommonControls.TextBoxBackgroundDisabled" Color="#EEEEF2"/>
    <SolidColorBrush x:Key="CommonControls.TextBoxBorderDisabled" Color="#CCCEDB"/>
    <SolidColorBrush x:Key="CommonControls.TextBoxTextDisabled" Color="#A2A4A5"/>

    <SolidColorBrush x:Key="CommonControls.TextBoxBackgroundHover" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="CommonControls.TextBoxBorderHover" Color="#007ACC"/>
    <SolidColorBrush x:Key="CommonControls.TextBoxTextHover" Color="#1E1E1E"/>

    <SolidColorBrush x:Key="CommonControls.TextBoxBackgroundFocused" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="CommonControls.TextBoxBorderFocused" Color="#007ACC"/>
    <SolidColorBrush x:Key="CommonControls.TextBoxTextFocused" Color="#1E1E1E"/>

    <SolidColorBrush x:Key="CommonControls.TextBoxTextInputSelection" Color="#007ACC"/>

    
</ResourceDictionary>